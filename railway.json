{
    "builds": [
      {
        "src": "client/",
        "use": "@railway/with-nodejs"
      },
      {
        "src": "server.js",
        "use": "@railway/with-nodejs"
      }
    ],
    "deployments": [
      {
        "src": "server.js",
        "dest": "/app"
      },
      {
        "src": "client/build",
        "dest": "/app/client/build"
      }
    ]
  }
  